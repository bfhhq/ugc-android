<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="config_sub_title">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">#ff0a5cad</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginTop">4.0dip</item>
        <item name="android:layout_marginBottom">3.0dip</item>
    </style>

    <style name="search_delete_history">
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">#ffffff</item>
    </style>


    <style name="comment_star_text">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/grade_text</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingRight">10dip</item>
    </style>

    <style name="regist_regbtn_style">
        <item name="android:textSize">@dimen/regist_regbtn_text_size</item>
        <item name="android:textColor">@color/common_white</item>
    </style>

    <style name="plot_text_style">
        <item name="android:textColor">@color/color_text_middle_gray</item>
    </style>

    <style name="Confirm_Window_Button" parent="BFYun.TextAppearance.Middle.White">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">42.666dp</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:background">@drawable/btn_confirm_window</item>
    </style>

    <style name="BFYun" />

    <!-- 文字默认显示 -->
    <style name="BFYun.TextAppearance">
        <item name="android:textColor">@color/text_default</item>
    </style>
    <!--文字超大字号 -->
    <style name="BFYun.TextAppearance.SuperLarge">
        <item name="android:textSize">@dimen/text_size_old_super_large</item>
    </style>
    <!--文字大字号 -->
    <style name="BFYun.TextAppearance.Large">
        <item name="android:textSize">@dimen/text_size_old_large</item>
    </style>
    <!--文字大字号 浅黑色字体 -->
    <style name="BFYun.TextAppearance.Large.LightBlack">
        <item name="android:textColor">@color/text_light_black</item>
    </style>

    <!--文字更小字号灰色字体 -->
    <style name="BFYun.TextAppearance.Smaller.Gray">
