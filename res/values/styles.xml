    </style>
    <style name="BFYun.TextAppearance.Smaller.Red">
        <item name="android:textColor">@color/text_red</item>
    </style>

    <!-- 图片大小设置处理 -->
    <style name="BFYun.ImageAppearance">
        <item name="android:scaleType">fitCenter</item>
        <item name="android:layout_width">@dimen/image_default_size_width</item>
        <item name="android:layout_height">@dimen/image_default_size_height</item>
    </style>

    <!--竖海报-->
    <style name="BFYun.ImageAppearance.Vertical">
        <item name="android:layout_width">@dimen/image_ve_size_width</item>
        <item name="android:layout_height">@dimen/image_ve_size_height</item>
    </style>

    <!--横向海报-->
    <style name="BFYun.ImageAppearance.Horizontal">
        <item name="android:layout_width">@dimen/image_ho_size_width</item>
        <item name="android:layout_height">@dimen/image_ho_size_height</item>
    </style>

    <!--横向海报-->
    <style name="BFYun.ImageAppearance.Center">
        <item name="android:layout_width">@dimen/image_center_size_width</item>
        <item name="android:layout_height">@dimen/image_center_size_height</item>
    </style>

    <!--大的横向海报-->
    <style name="BFYun.ImageAppearance.Horizontal.Big">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/image_big_ho_size_height</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    <!-- 图片大小处理结束 -->

    <!-- activity主题 出现时由中间变大 -->
    <style name="Theme.AppearLargerTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/Animation.Activity.Layer</item>
    </style>
    <!-- activity主题 出现时平移进出 -->
    <style name="Theme.AppearTranslateTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@style/Animation.Activity.Translate</item>
    </style>

    <!--activity 放大进入，变小左侧平移退出-->
    <style name="Animation.Activity.Layer" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/layer_larger_in</item>
        <item name="android:activityOpenExitAnimation">@anim/layer_smaller_left_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/layer_larger_in</item>
        <item name="android:activityCloseExitAnimation">@anim/layer_smaller_left_out</item>
    </style>

    <!--activity平移主题 右侧进入右侧退出，上一个activity左侧进入左侧退出-->
    <style name="Animation.Activity.Translate" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/push_left_in</item>
        <item name="android:activityOpenExitAnimation">@anim/push_left_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/push_right_in</item>
        <item name="android:activityCloseExitAnimation">@anim/push_right_out</item>
    </style>

    <style name="detetail_header_normal">
        <item name="android:textColor">@color/detail_page_header_text_normal</item>
        <item name="android:textSize">14dip</item>
    </style>

    <style name="detetail_header_selected">
        <item name="android:textColor">@color/detail_page_header_text_selected</item>
        <item name="android:textSize">14dip</item>
    </style>

    <!--半透明，带动画进入style-->
    <style name="transparent_activity">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
        <item name="android:windowBackground">@color/translucence_activity</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:colorForeground">#fff</item>
        <!--item name="android:backgroundDimEnabled">true</item--><!--模糊-->
        <!--item name="android:backgroundDimAmount">0.8</item-->
    </style>

    <!--半透明，不带动画进入style-->
    <style name="transparent_activity_no_animation">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@color/translucence_activity</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:colorForeground">#fff</item>
        <!--item name="android:backgroundDimEnabled">true</item--><!--模糊-->
        <!--item name="android:backgroundDimAmount">0.8</item-->
    </style>
    <!--titlebar三角标示 选择按钮风格 YQH 20140306-->
    <style name="titlebar_checkbox_theme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/titlebar_spinner_icon</item>
    </style>

    <!-- 弹出对话框背景样式 -->
    <style name="UpdateDialogBackgroundStyle"  parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <!--<item name="android:windowBackground">@android:color/transparent</item>-->
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:layout_width">@dimen/dialog_width</item>
        <item name="android:layout_height">@dimen/dialog_height</item>
        <item name="android:background">@drawable/dialog_update_bg</item>
        <item name="android:orientation">vertical</item>
        <item name="android:minWidth">@dimen/dialog_width</item>
        <item name="android:minHeight">@dimen/dialog_minheight</item>
        <item name="android:layout_marginLeft">12.6dip</item>
        <item name="android:layout_marginRight">12.6dip</item>
    </style>
    <!-- 对话框样 -->
    <style name="DialogStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <!--<item name="android:windowBackground">@android:color/transparent</item>-->
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <!-- 底部提示框 -->
    <style name="bottom_popup" parent="@android:style/Theme.Dialog">
        <item name="android:windowEnterAnimation">@anim/bottom_popup_in</item>
        <item name="android:windowExitAnimation">@anim/bottom_popup_out</item>
    </style>

